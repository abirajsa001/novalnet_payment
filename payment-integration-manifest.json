{
  "name": "novalnet-payment",
  "version": "1.0.0",
  "description": "This is a custom payment integration",
  "target": "payment",
  "endpoint": "https://novalnet-payment.vercel.app",
  "builders": {
    "paymentProvider": "1.0.0"
  },
  "routes": [
    {
      "path": "/payment-handler",
      "method": "POST",
      "secured": false
    },
    {
      "path": "/success",
      "method": "GET",
      "secured": false
    },
    {
      "path": "/failure",
      "method": "GET",
      "secured": false
    }
  ]
}
